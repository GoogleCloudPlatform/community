  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/functions-framework": "^1.1.0",
    "good-env": "^4.0.0",
    "redis": "^2.8.0",
    "redis-rate-limiter": "^1.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.13.6",
    "express": "^4.16.4",
    "gts": "^0.9.0",
    "ts-node": "^8.1.0",
    "tsc-watch": "^2.1.2",
    "typescript": "^3.4.4"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "yarn tsc -p .",
