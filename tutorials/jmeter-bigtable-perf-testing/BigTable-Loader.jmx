        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Write Installs" enabled="true">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import org.apache.hadoop.hbase.client.Connection;
import org.apache.hadoop.hbase.client.Table;
import org.apache.hadoop.hbase.util.Bytes;
import org.apache.hadoop.hbase.client.Put;
import org.apache.hadoop.hbase.TableName;
 
 
Connection btConn = props.get(&quot;btConn&quot;);
final Table table = btConn.getTable(TableName.valueOf(Bytes.toBytes(vars.get(&quot;app_installs_table&quot;))));
 
final byte[] COLUMN_FAMILY_NAME = Bytes.toBytes(&quot;cf1&quot;);
final long timestamp = System.currentTimeMillis();
 
Put put = new Put(Bytes.toBytes(vars.get(&quot;install_id&quot;)));
 
put.addColumn(COLUMN_FAMILY_NAME, Bytes.toBytes(&quot;owner&quot;), timestamp, Bytes.toBytes(vars.get(&quot;owner&quot;)));
put.addColumn(COLUMN_FAMILY_NAME, Bytes.toBytes(&quot;description&quot;), timestamp, Bytes.toBytes(vars.get(&quot;description&quot;)));
 
put.addColumn(COLUMN_FAMILY_NAME, Bytes.toBytes(&quot;app_status&quot;), timestamp, Bytes.toBytes(vars.get(&quot;app_status&quot;)));
put.addColumn(COLUMN_FAMILY_NAME, Bytes.toBytes(&quot;app_category&quot;), timestamp, Bytes.toBytes(vars.get(&quot;app_category&quot;)));
put.addColumn(COLUMN_FAMILY_NAME, Bytes.toBytes(&quot;created_ts&quot;), timestamp, Bytes.toBytes(vars.get(&quot;created_ts&quot;)));
 
table.put(put);
 
SampleResult.setRequestHeaders(put.toString());</stringProp>
        </JSR223Sampler>
